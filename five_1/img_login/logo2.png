                $('.v30h_mobile').removeClass('v30h_show');
                $('.v30h_mobile>dd').hide();
            });
        },
        // cart
        cart_toggle:function(){
            $('.v30hm_cartbox>dt').mouseenter(function(){
                global.getcartinfo();
                $('.v30hm_cartbox>dd').show();
